<settings>
    <servers>
        <server>
            <id>ossrh</id>
            <username>${{ secrets.OSSRH_USERNAME }}</username>
            <password>${{ secrets.OSSRH_TOKEN }}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>ossrh</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <gpg.executable>gpg</gpg.executable>
                <gpg.passphrase>${{ secrets.OSSRH_GPG_SECRET_KEY_PASSWORD }}</gpg.passphrase>
            </properties>
        </profile>
    </profiles>
</settings>